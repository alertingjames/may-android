<?xml version="1.0" encoding="utf-8"?>
<bookclub.technion.maymsgphoto.classes.MapWrapperLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/map_relative_layout"
    android:orientation="vertical"
    tools:context=".main.UserLocationViewActivity">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <fragment xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:map="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
        <ImageView
            android:id="@+id/back"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/back3"
            android:layout_marginBottom="120dp"
            android:layout_marginLeft="15dp"
            android:layout_gravity="bottom|left"/>
        <ImageView
            android:id="@+id/showInfo"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/show2"
            android:background="@drawable/light_blue_fill_round"
            android:layout_marginBottom="180dp"
            android:layout_marginLeft="15dp"
            android:layout_gravity="bottom|left"/>
    </FrameLayout>


</bookclub.technion.maymsgphoto.classes.MapWrapperLayout>
